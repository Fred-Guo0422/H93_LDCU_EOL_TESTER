    $Set_Channel_Number	=1; 			// channel is PWM ch
    $Generate_Signal_Freq 	= 100;		//产生的信号频率，以Hz为单位
    $Generate_Signal_Amp	=7;			//产生的信号赋值，以VPP为单位
    $Generate_Signal_DCYCle	=25;			//产生的方波占空比，在10Mhz以下波形，占空比设置范围为20~80
    call RIGOL_DG5072_Channel_Output();	// call 产生PWM 信号
   
    $rows_Loop=0;
    while($rows_Loop < $rows_Length)
    {
	// Loading Config info
	$rowArray 						= Array1DGetRowFrom2D($shell_Data_array, $rows_Loop); //Read row 
	$PWMIN_PIN					=Array1DGetValue($rowArray ,0);
	$PWMIN_Funtion					=Array1DGetValue($rowArray ,1);
	$PWMIN_SingalType				=Array1DGetValue($rowArray ,2);
	$PWMIN_Remark					=Array1DGetValue($rowArray ,3);
      
	$PWMIN_DUT_Command			=Array1DGetValue($rowArray ,6);
	$PWMIN_LoadBoxReset				=Array1DGetValue($rowArray ,7);
	$PWMIN_LoadBoxCommand			=Array1DGetValue($rowArray ,8);
	$Freq_Start_bit					=Array1DGetValue($rowArray ,9);
	$Duty_Start_bit					=Array1DGetValue($rowArray ,10);
	// LOAD LIMIT Start1(25%)
	$State1_duty					=Array1DGetValue($rowArray ,11);
	$State1_Freq_LSL				=Array1DGetValue($rowArray ,12);
	$State1_Freq_USL				=Array1DGetValue($rowArray ,13);
	$State1_Duty_LSL				=Array1DGetValue($rowArray ,14);
	$State1_Duty_USL				=Array1DGetValue($rowArray ,15);
       // LOAD LIMIT Start2 (75%)
	$State2_duty					=Array1DGetValue($rowArray ,16);
	$State2_Freq_LSL				=Array1DGetValue($rowArray ,17);
	$State2_Freq_USL				=Array1DGetValue($rowArray ,18);
